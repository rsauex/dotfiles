set $W Mod4
set $C Control
set $M Mod1
set $S Shift

font pango:Terminus 10

new_window pixel 2
new_float  pixel 2

for_window [class="(?i)gigolo"] floating enable
for_window [class="(?i)doublecmd"] floating enable
for_window [title="Double Commander( \[[0-9]+\])? [0-9]+.[0-9]+.[0-9]+ beta build [0-9]+; [0-9]{4}/[0-9]{2}/[0-9]{2}"] floating disable
for_window [class="(?i)lazarus"] floating enable

